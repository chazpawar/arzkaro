{
  "expo": {
    "name": "arzkaro",
    "slug": "arzkaro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "arzkaro",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
