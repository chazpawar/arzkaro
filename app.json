{
  "expo": {
    "name": "ArzKaro",
    "slug": "arzkaro",
    "version": "1.0.0",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#6C5CE7",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.arzkaro.app"
    },
    "android": {
      "package": "com.arzkaro.app",
      "adaptiveIcon": {
        "backgroundColor": "#6C5CE7"
      }
    },
    "web": {
      "bundler": "metro"
    }
  }
}
